  "bugs": {
    "url": "https://github.com/therebelrobot/diff-compare/issues"
  },
  "homepage": "https://github.com/therebelrobot/diff-compare#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.0.0",
    "codeclimate-test-reporter": "^0.1.0",
    "echint": "^1.4.0",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "standard": "^4.5.3",
    "trace-debug-log": "^1.0.1"
  }
